{
  "baseUrl": "https://github.com/luismayta/kubernetes-101",
  "ignorePatterns": [
    {
      "pattern": "^https://hadenlabs.com"
    },
    {
      "pattern": "^https://github.com/luismayta/kubernetes-101/compare"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/docs",
      "replacement": "{{BASEURL}}/docs"
    }
  ],
  "timeout": "20s",
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}
